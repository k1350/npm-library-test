{
  "name": "@k1350/npm-library-test",
  "version": "0.1.0-0",
  "private": false,
  "engines": {
    "pnpm": ">=8.15.1",
    "npm": "please_use_pnpm_instead"
  },
  "packageManager": "pnpm@8.15.3",
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/k1350/npm-library-test.git"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "exports": {
    "./test": {
      "import": "./dist/test/index.js",
      "require": "./dist/test/index.cjs",
      "types": "./dist/types/test/index.d.ts",
      "default": "./dist/test/index.js"
    },
    "./test2": {
      "import": "./dist/test2/index.js",
      "require": "./dist/test2/index.cjs",
      "types": "./dist/types/test2/index.d.ts",
      "default": "./dist/test2/index.js"
    },
    "./test3/child": {
      "import": "./dist/test3/child/index.js",
      "require": "./dist/test3/child/index.cjs",
      "types": "./dist/types/test3/child/index.d.ts",
      "default": "./dist/test3/child/index.js"
    }
  },
  "scripts": {
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "build": "tsup && tsc --project tsconfig.build.json",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/node": "^18.19.17",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
